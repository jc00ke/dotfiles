Host *
  IdentitiesOnly no
  IdentityAgent "~/.1password/agent.sock"
  ServerAliveInterval 120

Host heroku.jc00ke
  HostName heroku.com

Host *-github
    HostName github.com
    User git
    IdentitiesOnly yes

Host rp-github
    IdentityFile ~/.ssh/rp_auth.pub

Host ratio-github
    IdentityFile ~/.ssh/ratio_auth.pub

Host *-gitlab
    HostName gitlab.com
    User git
    IdentitiesOnly yes

Host rp-gitlab
    IdentityFile ~/.ssh/rp_auth.pub

Host ratio-gitlab
    IdentityFile ~/.ssh/ratio_auth.pub
